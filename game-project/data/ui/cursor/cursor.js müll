

// whoCanSell : [id - object, id - NPC], 
// whoCanBuy : [id - object, id - NPC],


//{id : 0, type : "", subtype : "", name : "", uniqueness : false, picture : "",   armorClass : [], strength : "", requirements : {},  {},                 magicProperties : {}, price : 20,  damage : [1, 2, 3, 4, 5, 6], charges : "", whoCanSell : [], whoCanBuy : [], },               magicProperties : {}, price : 20,  damage : [1, 2, 3, 4, 5, 6],                                       charges : null,  },


// belt : false,  - можно поместить в пояс
// cursor : true, - используеться курсором 
// activity : true - активен или нет этот предмет
// cells : [1, 1] - колличество занимаемых ячеек (null - нельзя поместить в инвентарь)
// texture : "objcurs_1.png", - текстура
// action: "", - действие
// type : "", - тип предмета
// name : "", - название
// description : "cursorMain",  - описание
// textureObjectId - номер текстуры,

// id - номер, type - тип предмета, linkId - его номер в нужном нам массиве, description - описание
